                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "properties": {
                                            "id": {
                                                "type": "integer",
                                                "example": 1
                                            },
                                            "parent_id": {
                                                "type": "integer",
                                                "example": null,
                                                "nullable": true
                                            },
                                            "name": {
                                                "type": "string",
                                                "example": "Yataq d…ôsti"
                                            },
                                            "sku": {
                                                "type": "string",
                                                "example": "321231232"
                                            },
                                            "description": {
                                                "type": "string",
                                                "example": "ttttt"
                                            },
                                            "price": {
                                                "type": "string",
                                                "example": "10.00"
                                            },
                                            "stock": {
                                                "type": "integer",
                                                "example": 20
                                            },
                                            "discount": {
                                                "type": "string",
                                                "example": null,
                                                "nullable": true
                                            },
                                            "discount_ends_at": {
                                                "type": "string",
                                                "example": null,
                                                "nullable": true
                                            },
                                            "is_set": {
                                                "type": "integer",
                                                "example": 1
                                            },
                                            "created_at": {
                                                "type": "string",
                                                "example": null,
                                                "nullable": true
                                            },
                                            "updated_at": {
                                                "type": "string",
                                                "example": null,
                                                "nullable": true
                                            },
                                            "images": {
                                                "type": "array",
                                                "items": {
                                                    "type": "object"
                                                }
                                            },
                                            "attribute_values": {
                                                "type": "array",
                                                "items": {
                                                    "type": "object"
                                                }
                                            },
                                            "variations": {
                                                "type": "array",
                                                "items": {
                                                    "type": "object"
                                                }
                                            }
                                        },
                                        "type": "object"
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
